<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GenshinMobileApp.MainPage">

    <StackLayout>
        <ImageButton 
            x:Name="GuidesButton" 
            Source="MainPageGuides.png" 
            HorizontalOptions="Center"
            Clicked="GuidesButton_Clicked">
        </ImageButton>
        <ImageButton
            x:Name="WishesButton"
            Source="MainPageWishes.png"
            HorizontalOptions="Center"
            Clicked="WishesButton_Clicked">
        </ImageButton>
    </StackLayout>
</ContentPage>
